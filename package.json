{
  "name": "generator-lean-react",
  "version": "0.1.3",
  "description": "React and React Native Conventions for Lean Iterations LLC",
  "homepage": "https://github.com/leaniterationsllc/generator-lean-react",
  "author": {
    "name": "Aaron Cordova",
    "email": "acordova05@gmail.com",
    "url": "505aaron"
  },
  "files": ["generators"],
  "main": "generators/index.js",
  "keywords": ["react", "react-native", "yeoman-generator"],
  "devDependencies": {
    "eslint": "^4.1.0",
    "eslint-config-prettier": "^2.4.0",
    "eslint-config-xo": "^0.18.2",
    "eslint-plugin-prettier": "^2.2.0",
    "husky": "^0.14.3",
    "jest": "^22.0.6",
    "lint-staged": "^4.2.1",
    "nsp": "^3.1.0",
    "prettier": "^1.7.0",
    "yeoman-assert": "^3.1.0",
    "yeoman-test": "^1.7.0"
  },
  "dependencies": {
    "chalk": "^2.1.0",
    "yeoman-generator": "2.0.5",
    "yosay": "^2.0.1"
  },
  "jest": {
    "testEnvironment": "node",
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": -10
      }
    }
  },
  "scripts": {
    "prepublish": "nsp check",
    "pretest": "eslint .",
    "precommit": "lint-staged",
    "test": "jest"
  },
  "lint-staged": {
    "*.js": ["eslint --fix", "git add"],
    "*.json": ["prettier --write", "git add"]
  },
  "eslintConfig": {
    "extends": ["xo", "prettier"],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 90
        }
      ]
    },
    "plugins": ["prettier"]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/leaniterationsllc/generator-lean-react"
  },
  "license": "Apache-2.0"
}
